shader_directory=/shaders
sheet_directory=/spritesheets
img_directory=/spritesheets/images/
font_directory=/fonts




//	Shaders
//	Shader name						shader file

shader=__TEXT_SHADER__				file=text.shader
shader=std_strict_batch				file=batch_test.shader
shader=std_soft_batch				file=sb_std.shader
shader=std_floating_quads			file=sb_floating_quads.shader
shader=std_free_batch				file=fb_std.shader
shader=std_ui_batch					file=uib_std.shader



//	Sprite sheets
//	Sheet name						image file				sprites					shader name
sheet=fox							img=test.png			cfg=test.cfg			shader_name=std_strict_batch
sheet=soft_batch_sheet1				img=test.png			cfg=test.cfg			shader_name=std_soft_batch
sheet=fqsoft_batch_sheet1			img=test.png			cfg=test.cfg			shader_name=std_floating_quads
sheet=fqsoft_batch_sheet2			img=panda.png			cfg=panda.cfg			shader_name=std_floating_quads
sheet=free_batch_sheet1				img=test.png			cfg=test.cfg			shader_name=std_free_batch
sheet=free_batch_sheet2				img=panda.png			cfg=panda.cfg			shader_name=std_free_batch
sheet=ui_batch_sheet				img=ui.png				cfg=gui.cfg				shader_name=std_ui_batch



//	Fonts
//	Font name						Font image					(o)font sheet splits
font=font_cyrillic					img=cyrillic.png			w=9		h=9

unknown_symbol_replacement_index=30
space_index=30



//	UI-related data


//	Pane skin
pane=default
pane_corner_len=20
pane_ul=ui_batch_sheet:ul_corner
pane_ur=ui_batch_sheet:ur_corner
pane_ll=ui_batch_sheet:ll_corner
pane_lr=ui_batch_sheet:lr_corner


pane_left=ui_batch_sheet:leftborder
pane_right=ui_batch_sheet:rightborder
pane_upper=ui_batch_sheet:upperborder
pane_lower=ui_batch_sheet:lowerborder

pane_mid=ui_batch_sheet:middle


//	Standard button

button=default
button_corner_len=5
button_ul=ui_batch_sheet:
button_ur=ui_batch_sheet:
button_ll=ui_batch_sheet:
button_lr=ui_batch_sheet:


button_left=ui_batch_sheet:
button_right=ui_batch_sheet:
button_upper=ui_batch_sheet:
button_lower=ui_batch_sheet:

button_mid=ui_batch_sheet:


//	Pressed button

button=pressed
button_corner_len=5
button_ul=ui_batch_sheet:
button_ur=ui_batch_sheet:
button_ll=ui_batch_sheet:
button_lr=ui_batch_sheet:


button_left=ui_batch_sheet:
button_right=ui_batch_sheet:
button_upper=ui_batch_sheet:
button_lower=ui_batch_sheet:

button_mid=ui_batch_sheet:


