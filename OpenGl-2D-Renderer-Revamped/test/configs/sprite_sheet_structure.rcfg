shader_directory=/shaders
sheet_directory=/spritesheets
img_directory=/spritesheets/images/





//	Shaders
//	Shader name						shader file

shader=__TEXT_SHADER__				file=text.shader
shader=std_strict_batch				file=batch_test.shader
shader=std_soft_batch				file=sb_std.shader
shader=std_floating_quads			file=sb_floating_quads.shader
shader=std_free_batch				file=fb_std.shader
shader=std_ui_batch					file=uib_std.shader



//	Sprite sheets
//	Sheet name						image file				sprites					shader name
sheet=fox							img=test.png			cfg=test.cfg			shader=std_strict_batch
sheet=soft_batch_sheet1				img=test.png			cfg=test.cfg			shader=std_soft_batch
sheet=fqsoft_batch_sheet1			img=test.png			cfg=test.cfg			shader=std_floating_quads
sheet=fqsoft_batch_sheet2			img=panda.png			cfg=panda.cfg			shader=std_floating_quads
sheet=free_batch_sheet1				img=test.png			cfg=test.cfg			shader=std_free_batch
sheet=free_batch_sheet2				img=panda.png			cfg=panda.cfg			shader=std_free_batch
sheet=ui_batch_sheet				img=ui.png				cfg=gui.cfg				shader=std_ui_batch



//	Fonts
//	Font name						Font image					(o)font sheet splits
font=font_cyrillic					img=cyrillic.png			w=9		h=9
offsets=20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20, 20
